/* pages/goodsdetail/index.wxss */
/* 滑动滚轮 */
.swiper-box .wx-swiper-dots.wx-swiper-dots-horizontal{
     margin-bottom: 2rpx;
}
.swiper-box .wx-swiper-dot{
    width:40rpx;
    display: inline-flex;
    height: 10rpx;
    margin-left: 20rpx;
    justify-content:space-between;
}
.swiper-box .wx-swiper-dot::before{
    content: '';
    flex-grow: 1; 
    background: rgba(255,255,255,0.8);
    border-radius: 8rpx
}
.swiper-box .wx-swiper-dot-active::before{
    background:#009009;   
}

.titlecan{
  display: block;
  padding: 20rpx;
  overflow: auto;
  padding-bottom: 20rpx;
}
.goodname{
  float: left;
}
.money{
  float: right;
  color: #ff0000;
  font-weight: 600;
}
.goodsdesc{
  display: block;
  overflow: auto;
  padding: 20rpx;
}
.desctext{
  display: block;
  overflow: auto;
  padding-bottom: 10rpx;
}
.desctitle{
  display:block;
  color:#ccc;
  margin-bottom: 10rpx;
}
.con{
  display: block;
  overflow: auto;
  font-size: 30rpx;
  text-indent: 2em;
  line-height:40rpx;
}
.imgdesccan{
  padding:10rpx;
}
.imgcan{
  border:1rpx solid #ddd;
  padding:20rpx;
  border-radius: 10rpx;
}
.descimg{
  display: block;
  width: 80%;
  margin:auto;
  padding: 30rpx;
  border:2rpx dotted #ddd;
  max-height: 500rpx;
  margin-bottom: 50rpx;
}
.gooddetailcon{
  padding-bottom: 140rpx;
}
.detailfoot{
  position: fixed;
  bottom: 0;
  width: 100%;
  height:130rpx;
  line-height: 130rpx;
  background: #eee;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.addbtn{
  width: 30%;
  height:100%;
  color: #fff;
  background-color: hsl(41, 100%, 50%);
  margin: 0;
  border-radius: 0;
  line-height: 130rpx;
  font-size: 40rpx;
}
.addbtn:hover{
  background: #f0a400;
}
.carnum{
  color:#ff0000;
  position: absolute;
  font-weight: 900;
  right:10rpx;
  bottom:0;
  z-index: 10
}
.orderbtn{
  width: 30%;
  height:100%;
  color: #ffffff;
  background-color: #ff2600;
  margin: 0;
  border-radius: 0;
  line-height: 50rpx;
  font-size: 40rpx;
  padding-top: 20rpx;
}
.smalltext{
  display: block;
  font-size: 23rpx;
}
.goodicon{
  position: relative;
  margin-left: 30rpx;
  width: 80rpx;
  height:80rpx;
  background-size: cover;
  background-image: url('data:image/png;base64,')
}
