/* pages/aboutme/index.wxss */
.mecan{
  width: 100%;
  height:auto;
  overflow: auto;
  margin-top:0;
}
.personalme{
  display: flex;
  justify-content: left;
  align-items: center;
  margin:auto;
  margin-bottom: 40rpx;
  padding-bottom: 40rpx;
  width: 90%;
  border-bottom: 6rpx dotted #eee;
}
.myicon{
  width: 200rpx;
  height:200rpx;
  border:6rpx solid #eee;
  border-radius: 15rpx;
}
.myname{
  margin-left:35rpx;
  color: rgb(136, 132, 132);
}
.myname text{
  display: block;
  margin-top:20rpx;
}
.models{
  padding:20rpx 10rpx;
  background-color: #eee;
}
.models navigator{
  display: flex;
  justify-content: space-between;
  padding-bottom:15rpx;
  margin-bottom: 15rpx;
  margin-left: 30rpx; 
  margin-right: 30rpx;
  border-bottom: 5rpx solid #fff;
}
.models navigator:last-child{
  padding-bottom:0;
  margin-bottom: 0;
  border-bottom: 0rpx solid #fff;
}
.models text{
  font-size: 34rpx;
}